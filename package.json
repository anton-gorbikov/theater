{
  "name": "theater",
  "version": "1.0.0",
  "description": "The site about theaters",
  "scripts": {
    "postinstall": "dotnet tool restore",
    "build:web": "dotnet fable src/app/app.fsproj -o dist/tmp --run webpack",
    "build:api": "node api-prepare && dotnet fable src/api -o dist/api",
    "start": "dotnet fable watch src/app/app.fsproj -o dist/tmp --run webpack serve",
    "start:web": "yarn build:web && node dist/app/index.js",
    "start:api": "yarn build:api && node dist/api/main.js"
  },
  "author": "Anton Gorbikov",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.1",
    "html-webpack-plugin": "^5.3.1",
    "postcss-loader": "^5.2.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
}
